{
  "name": "minimalist_react",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "scripts": {
    "test": "$(npm bin)/jest",
    "build": "$(npm bin)/parcel build ./src/index.html --no-source-maps",
    "start": "$(npm bin)/tsc --noEmit --watch & $(npm bin)/parcel ./src/index.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/styles": "^4.9.0",
    "@reduxjs/toolkit": "^1.2.5",
    "classnames": "^2.2.6",
    "css.gg": "^1.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.8.3",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "react-test-renderer": "^16.8.6",
